{
  "manifest_version": 3,
  "name": "WiseShield - Phishing Protection",
  "version": "1.1.0",
  "description": "Real-time phishing detection and protection powered by AI",
  "author": "WiseShield Security Team",
  "homepage_url": "https://wiseshield.example.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "tabs",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "utils/storage.js",
        "utils/api.js",
        "utils/helpers.js",
        "utils/form-protection.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "settings/settings.html",
  "web_accessible_resources": [
    {
      "resources": [
        "warning/warning.html",
        "onboarding/onboarding.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}